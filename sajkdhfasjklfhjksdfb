-- Scripts:

local function NUMUH_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	frame = script.Parent.frame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(NUMUH_fake_script)()
local function BYZYDYT_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local frame = script.parent.frame
	local Open =script.parent.Open
	
	Open.MouseButton1Up:Connect(function()
		-- Open Inventory
	
		    frame:TweenPosition(
			UDim2.new(-0, 0, 0.569, 0), -- end Position 
			"In", -- Easing Direction
			"Bounce", -- Easing Style
			1.5, -- Time In Seconds
			false -- Override Any Other Tweens
		)
	end)
	
end
coroutine.wrap(BYZYDYT_fake_script)()
local function MURSHU_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local frame = script.parent.frame
	local Close =script.parent.Close
	
	Close.MouseButton1Up:Connect(function()
		-- Open Inventory
	
		frame:TweenPosition(
			UDim2.new(0.409, 0, -0.99, 0), -- end Position 
			"In", -- Easing Direction
			"Back", -- Easing Style
			2.5, -- Time In Seconds
			true -- Override Any Other Tweens
		)
	end)
	
end
coroutine.wrap(MURSHU_fake_script)()

Loop.MouseButton1Click:connect(function()

	local field = PlayerName.Text

	local LocalUser = game.Players.LocalPlayer
	local ENEMY = field

	for i = 1,4000 do
		game.ReplicatedStorage.Remotes.Human_Punch:FireServer(LocalUser.Character.RightHand, "RightPunch", workspace[ENEMY].UpperTorso,5,true,"RightPunch",game.Players.LocalPlayer.Backpack.Boxing.Handle.Hit,5)
		wait()
	end
end)

PlayerName.FocusLost:connect(function()
	for i,v in pairs(game.Players:GetChildren()) do
		if (string.sub(string.lower(v.Name),1,string.len(PlayerName.Text))) == string.lower(PlayerName.Text) then
			PlayerName.Text = v.Name
		end
	end
end)

LoopAll.MouseButton1Click:connect(function()
	local children = game.Players:GetChildren()

	local LocalUser = game.Players.LocalPlayer
	local NAME = game.Players.LocalPlayer.Name
	for i = 1, #children do
		if(children[i].Name ~= NAME) then
			for a = 1,100 do
				game.ReplicatedStorage.Remotes.Human_Punch:FireServer(LocalUser.Character.LeftHand, "LeftPunch", children[i].Character.LeftHand, 5, true, "RightPunch", LocalUser.Character.Boxing.Handle.Hit, 5)
			end
		end
	end
end)
